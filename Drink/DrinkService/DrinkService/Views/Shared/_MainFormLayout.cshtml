<script>
    $(document).ready(function () {
        $(".back-link-main").bind("click", function () {
            smat.service.closeForm({
                contentId: "body_id"
            });
            //showLimitLeart();
        });

    });

</script>

<section id="body_id" class="vbox" style="height:100%;">
    <header class="bg-white-only header header-md navbar navbar-fixed-top-xs">
        <ul class="nav navbar-nav hidden-xs">
            <li>
                <a href="#" id="back-link" class="text-muted back-link-main">
                    <i class="icon icon-home text"></i>
                </a>
            </li>
        </ul>
        <div class="navbar-form navbar-left input-s-lg m-t m-l-n-xs hidden-xs" role="search">
            <div class="form-group">
                <span class="nav-title">@ViewBag.Title</span>
                @if (ViewBag.isTestVersion as string == "test")
                {
                    <span style="color:red;font-weight:bold;">テスト環境</span>

                }else if (ViewBag.isTestVersion as string == "kensyu")
                    {
                        <span style="color:red;font-weight:bold;">研修環境</span>

                    }
            </div>
        </div>
        <div class="navbar-right " style="position: absolute;right: 0;background-color: #fff;">
            <ul class="nav navbar-nav m-n hidden-xs nav-user user">
                <li class="dropdown">
                    <a href="#" class="dropdown-toggle bg clear" data-toggle="dropdown">
                        <span class="thumb-sm avatar pull-right m-t-n-sm m-b-n-sm m-l-sm">
                            <img src="~/SMAT.UI/images/temp-user.png" alt="...">
                        </span>
                        @((ViewBag.LoginUser as DrinkService.Data.Models.UserSession).ShopName) &nbsp;&nbsp;
                        @((ViewBag.LoginUser as DrinkService.Data.Models.UserSession).StaffName)<b class="caret"></b>
                    </a>
                    <ul class="dropdown-menu animated fadeInRight">
                        <li>
                            <span class="arrow top"></span>
                            <a href="~/Main/PasswordChange">パスワード設定</a>
                        </li>
                        <li class="divider"></li>
                        <li>
                            <a href="~/Main/Logout">ログアウト</a>
                        </li>
                    </ul>
                </li>
            </ul>
        </div>
    </header>
    <section>
        <section class="hbox stretch">
            <section id="content">
                <section class="vbox">
                    <!-- contentBody -->
                    @RenderBody()
                    <!-- /contentBody -->
                </section>
            </section>
        </section>
    </section>
</section>
<script>
    $(document).ready(function () {
        smat.service.uiAfterInit("#body_id");
    });
</script>