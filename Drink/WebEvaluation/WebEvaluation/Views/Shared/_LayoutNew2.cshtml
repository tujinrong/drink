        $(document).ready(function () {
            var msg = "@ViewBag.msg";
            var msgType = "@ViewBag.msgType";
            if (msg.length > 0) {
                if (msgType.length > 0) {
                    SMAT.Service.toastr(msg,msgType);
                } else {
                    SMAT.Service.toastr(msg);
                } 
            }

            // var timer = setInterval(keepSession, 60000);

            $(document).on('change', 'table thead [type="checkbox"]', function (e) {
                e && e.preventDefault();
                var $table = $(e.target).closest('div.s-grid').find(' table:first'), $checked = $(e.target).is(':checked'), $colIndex = $(e.target).attr('colIndex');
                $('tbody [type="checkbox"][colIndex="' + $colIndex + '"]', $table).prop('checked', $checked);
            });

        });

        function keepSession(){
            $.ajax({
                url: "@Url.Action("KeepSession", "Home")",
