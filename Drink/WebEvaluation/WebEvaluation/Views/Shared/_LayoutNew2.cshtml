    <div id="s_form_content" class="container body-content s-form-content" style="width:1266px; margin-right:auto;margin-left:auto;">
        @RenderBody()
    </div>

    @RenderSection("scripts", required: false)

    <script type="text/javascript">

        $(document).ready(function () {


            $("#link_home").bind('click', function (e) {
                var pages = $(".s-dy-page");
                $.each(pages, function () {
                    if ($(this).attr("id")) {
                        smat.service.closeForm({
                            contentId: $(this).attr("id")
                        });
                    }
                    
                })
                
               
            })

            var forms = $('form');

            $.each(forms, function () {
                var form = $(this);
                var ctrls = $(this).find('input , select');
                if (ctrls.length > 0) {
                    ctrls.last().addClass('form-last-input')
                }

                var btns = $(this).find('input');

                $.each(btns, function () {
                    $(this).bind('keydown', function (e) {
                        if ($(this).hasClass('form-last-input')) {
                            return;
                        }
                        if (window.event.keyCode == 13) {
                            window.event.keyCode = 9;
                        }
                    });
                });

                var selects = $(this).find('select');

                $.each(selects, function () {
                    $(this).bind('keydown', function (e) {
                        if (window.event.keyCode == 13) {
                            if ($(this).hasClass('form-last-input')) {
                                form.submit();
                                return;
                            }
                            window.event.keyCode = 9;
                        }
                    });
                });
            });

            //if ($(window).height() < $(document).height()) {
            //    $("#footer-bar").css('position','relative');
            //    $("#footer-bar").css('margin-bottom','-20px');
            //}

            var pages = $('.pagination-container');

            $.each(pages, function () {
                var lis = $(this).find('li');
                if (lis.length == 1) {
                    $(this).hide();
                    $(this).after("<div class='col-lg-13' style='padding-left:0px'><font>&nbsp;</font></div>");
                }
                
            });
            
        });
    </script>
</body>
