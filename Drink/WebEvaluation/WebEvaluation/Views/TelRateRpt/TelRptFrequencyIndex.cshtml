@model IEnumerable<WebEvaluation.ViewModels.TelRateRptViewModel>
@using PagedList.Mvc;
@{
    ViewBag.IMG = "../../Content/icon/m05.png";
    ViewBag.Title = "通話率一覧";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="jumbotron">
<!--通話率一覧-->
<form class="form-horizontal">
    <div class="form-group">
        <div class="col-lg-12 control-label">
            @Html.OpenWindow("プレビュー", "/TelRptFrequencyIndexPreview", "1440", "800", new { Time = ViewBag.Time, Year = ViewBag.Year, Season = ViewBag.Season, Months = ViewBag.Months, DateFrom = ViewBag.DateFrom, DateTo = ViewBag.DateTo,Range = ViewBag.Range, divisionCD = ViewBag.divisionCD, ShopCD = ViewBag.ShopCD, StaffCD = ViewBag.StaffCD, Sort = ViewBag.Sort }, new { @class = "btn btn-info btn-sm" })
            @Html.ButtonAction(true, "CSV出力", "/csv", new { Time = ViewBag.Time, Year = ViewBag.Year, Season = ViewBag.Season, Months = ViewBag.Months, DateFrom = ViewBag.DateFrom, DateTo = ViewBag.DateTo,Range = ViewBag.Range, divisionCD = ViewBag.divisionCD, ShopCD = ViewBag.ShopCD, StaffCD = ViewBag.StaffCD, Sort = ViewBag.Sort }, new { @class = "btn btn-success btn-sm" })
            @Html.ButtonAction(false, "再検索", "TelRptFrequencySearch", new { Time = ViewBag.Time, Year = ViewBag.Year, Season = ViewBag.Season, Months = ViewBag.Months, DateFrom = ViewBag.DateFrom, DateTo = ViewBag.DateTo,Range = ViewBag.Range, divisionCD = ViewBag.divisionCD, ShopCD = ViewBag.ShopCD, StaffCD = ViewBag.StaffCD, Sort = ViewBag.Sort }, new { @class = "btn btn-primary btn-sm" })
        </div>
    </div>
</form>
    <table class="table table-condensed table-bordered table-hover text-center" border="1">
        <thead>
            <tr style="background-color:#e9e9e9">
                <th class="text-center" colspan="4"></th>
                <th class="text-center" colspan="4">一回目</th>
                <th class="text-center" colspan="4">二回目</th>
                <th class="text-center" colspan="4">三回目</th>
            </tr>
            <tr style="background-color:#e5e5e5">
                <th class="text-center">事業部</th>
                <th class="text-center">グループ名</th>
                <th class="text-center">店舗名</th>
                <th class="text-center">担当</th>
                <th class="text-center">総数</th>
                <th class="text-center">繋がった</th>
                <th class="text-center">繋がらず</th>
                <th class="text-center">通話率</th>
                <th class="text-center">総数</th>
                <th class="text-center">繋がった</th>
                <th class="text-center">繋がらず</th>
                <th class="text-center">通話率</th>
                <th class="text-center">総数</th>
                <th class="text-center">繋がった</th>
                <th class="text-center">繋がらず</th>
                <th class="text-center">通話率</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr class="" style="background-color:white">
                    <td class="text-left">@Html.DisplayFor(modelItem => item.DivName)</td>
                    <td class="text-left">@Html.DisplayFor(modelItem => item.GroupName)</td>
                    <td class="text-left">@Html.DisplayFor(modelItem => item.ShopCD)</td>
                    <td class="text-left">@Html.DisplayFor(modelItem => item.StaffName)</td>
                    <td class="text-right">@Html.DisplayFor(modelItem => item.FirstCount)</td>
                    <td class="text-right">@Html.DisplayFor(modelItem => item.FirstConnected)</td>
                    <td class="text-right">@Html.DisplayFor(modelItem => item.FirstNoConnected)</td>
                    <td class="text-right">@Html.DisplayFor(modelItem => item.FirstRate)</td>
                    <td class="text-right">@Html.DisplayFor(modelItem => item.SecondCount)</td>
                    <td class="text-right">@Html.DisplayFor(modelItem => item.SecondConnected)</td>
                    <td class="text-right">@Html.DisplayFor(modelItem => item.SecondNoConnected)</td>
                    <td class="text-right">@Html.DisplayFor(modelItem => item.SecondRate)</td>
                    <td class="text-right">@Html.DisplayFor(modelItem => item.ThirdCount)</td>
                    <td class="text-right">@Html.DisplayFor(modelItem => item.ThirdConnected)</td>
                    <td class="text-right">@Html.DisplayFor(modelItem => item.ThirdNoConnected)</td>
                    <td class="text-right">@Html.DisplayFor(modelItem => item.ThirdRate)</td>
                </tr>
            }
        </tbody>
    </table>

</div>
